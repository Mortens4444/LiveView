<Project Sdk="WixToolset.Sdk/5.0.2">
  <PropertyGroup>
    <PackageCompression>Embed</PackageCompression>
    <TargetFramework>net462</TargetFramework>
    <DisableEmbeddedUI>true</DisableEmbeddedUI>
    <SuppressWixUIValidation>true</SuppressWixUIValidation>
    <SuppressAllWarnings>false</SuppressAllWarnings>
    <Pedantic>true</Pedantic>
    <DefineConstants>
      Company=$(Company);
      ShortVersion=$(ShortVersion);
      ProgramVersion=$(ProgramVersion);
      LvFilesSource=$(SolutionDir)LiveView\bin\Release\net462;
    </DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\LiveView\LiveView.csproj">
      <TargetFrameworks>net9.0-windows,net462</TargetFrameworks>
    </ProjectReference>
  </ItemGroup>
</Project>